{
  "name": "receipt-ms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/unit/",
    "test:happy-paths": "node tests/integration/happy-paths.test.js",
    "test:error-cases": "node tests/integration/error-cases.test.js",
    "test:integration": "node tests/run-all-tests.js",
    "test:all": "node tests/run-all-tests.js",
    "test:mcp": "echo 'Starting MCP testing - use Chrome DevTools MCP to test the application' && npm run dev",
    "test:mcp:performance": "node tests/mcp/performance-testing.js",
    "test:mcp:automation": "node tests/mcp/automation-testing.js",
    "test:mcp:debugging": "node tests/mcp/debugging-testing.js",
    "test:mcp:advanced": "node tests/mcp/run-advanced-mcp-tests.js",
    "mcp:install": "npx chrome-devtools-mcp@latest --help",
    "mcp:start": "npx chrome-devtools-mcp@latest --logFile /tmp/mcp-test.log",
    "mcp:test": "echo 'Testing MCP integration...' && curl -s http://localhost:5174 > /dev/null && echo '✅ App is running at http://localhost:5174' || echo '❌ App not running - start with npm run dev'"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "chrome-devtools-mcp": "^0.4.0",
    "jsdom": "^27.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-table": "^8.21.3",
    "autoprefixer": "^10.4.21",
    "fuse.js": "^7.1.0",
    "postcss": "^8.5.6",
    "react-hook-form": "^7.63.0",
    "react-router-dom": "^7.9.3",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  }
}
